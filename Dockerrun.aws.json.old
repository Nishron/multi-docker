{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "client",
            "image": "nishron/multi-client",
            "hostname": "client",
            "essential": false
        },
        {
            "name": "server",
            "image": "nishron/multi-server",
            "hostname": "api",
            "essential": false
        },
        {
            "name": "worker",
            "image": "nishron/multi-worker",
            "hostname": "worker",
            "essential": false
        },
        {
            "name": "nginx",
            "image": "multi-nginx",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ],
            "links": ["client", "server"]
        }
    ]
}